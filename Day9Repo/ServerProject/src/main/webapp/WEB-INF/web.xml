<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>welcomeServlet</servlet-name>
  	<display-name>welcomeServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.welcomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmpoyeeServlet</servlet-name>
  	<display-name>EmpoyeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.employeeProject.EmpoyeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>welcomeServlet</servlet-name>
  	<url-pattern>/welcomeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmpoyeeServlet</servlet-name>
  	<url-pattern>/EmpoyeeServlet</url-pattern>
  </servlet-mapping>
</web-app>
